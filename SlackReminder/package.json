{
  "name": "slack-reminder",
  "version": "0.0.1",
  "description": "SlackReminder scenario",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "module": "dist/esm/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@bearer/core": "0.27.1",
    "@bearer/intents": "0.27.3",
    "@bearer/ui": "0.27.1",
    "axios": "^0.18.0",
    "@stencil/core": "0.10.9"
  },
  "devDependencies": {
    "@bearer/bearer-cli": "0.27.4",
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
